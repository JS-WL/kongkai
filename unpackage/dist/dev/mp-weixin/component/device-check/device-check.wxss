.device_check_wrap {
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.device_check_wrap .check_btn_wrap {
    width: 100%;
    padding-top: 50rpx;
    height: 88rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
}
.device_check_wrap .check_btn_wrap .check_btn {
      width: 294rpx;
      height: 88rpx;
      background: url(data:image/png;base64,) no-repeat;
      background-size: 100%;
      border: none;
      font-size: 36rpx;
      border-radius: 999rpx;
      color: #fff;
      box-sizing: border-box;
      padding: 0 30rpx;
      text-align: center;
      line-height: 88rpx;
}

